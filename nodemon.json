{
	"restartable": "rs",
	"ignore": [
	  ".git",
	  "node_modules/**/node_modules"
	],
	"verbose": true,
	"execMap": {
	  "js": "node --harmony"
	},
	"events": {
	  "restart": "echo \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
	},
	"watch": [
		"src/",
		"test/",
		"src/modules",
		"src/modules/grammar"
	],
	"env": {
	  "NODE_ENV": "development"
	},
	"ext": "js,ts,ne,json"
}